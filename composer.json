{
    "name": "mobicms-modules/expressive-demo-app",
    "description": "Demo application",
    "type": "mobicms-module",
    "homepage": "https://mobicms.org",
    "license": "GPL-3.0-only",
    "keywords": [
        "mobicms",
        "demo"
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "mobicms/system": "dev-develop"
    },
    "require-dev": {
        "mobicms/dummy": "dev-master",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Mobicms\\Modules\\ExpressiveDemoApp\\": "src/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
